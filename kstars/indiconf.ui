    <tabstop>timeCheck</tabstop>
    <tabstop>GeoCheck</tabstop>
    <tabstop>crosshairCheck</tabstop>
    <tabstop>messagesCheck</tabstop>
    <tabstop>buttonOk</tabstop>
    <tabstop>buttonCancel</tabstop>
</tabstops>
